  <PropertyGroup>
    <PackageId>PureQL.CSharp.Model.Serialization</PackageId>
    <RootNamespace>PureQL.CSharp.Model.Serialization</RootNamespace>
    <AssemblyName>PureQL.CSharp.Model.Serialization</AssemblyName>
    <Authors>Dmitry Kurochkin</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
